            <Button fx:id="citibtnClose" mnemonicParsing="false" text="닫기" />
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="113.0" layoutY="228.0" prefHeight="137.0" prefWidth="194.0" spacing="10.0">
         <children>
            <TextField fx:id="cititextId1" prefHeight="39.0" prefWidth="82.0" promptText="호 수" />
